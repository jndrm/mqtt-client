{
    "name": "drmer/mqtt-client",
    "description": "Mqtt client in PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Donald",
            "email": "donald@drmer.net"
        }
    ],
    "require": {
        "drmer/mqtt-packet": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Drmer\\Mqtt\\Client\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Drmer\\Tests\\Mqtt\\Client\\": "tests"
        }
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "../packet/"
        }
    }
}
